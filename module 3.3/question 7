#include<stdio.h>
int main()
{
    char s[30];
    int i,count=0;
    printf("Enter the string :");
    scanf("%s",&s);
    for (i = 0;s[i]!=0; i++)
    {
        count++;
    }
   /* while(num>0)
    {
    	num = num / 10;
        count++;
	}*/
    printf("Lenght of string is %d",count);
	return 0;
}
