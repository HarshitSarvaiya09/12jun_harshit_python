  #include<iostream>
 using namespace std;
 class base
 {
     public:
     int no;
     string name;
     int getdata()
     {
         cout<<"enter your no:";
         cin>>no;
         cout<<"enter your name:";
         cin>>name;
     }
 };
 class derived : public base
 {
     public:
     void display()
     {
         cout<<name<<" your no is:"<<no;
     }
     
 };
 int main()
 {
     derived d1;
     d1.getdata();
     d1.display();
 }
 
